<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>ID</th>
    <th>Name</th>
  </tr>

  <% BoxStyle.all.each do |box_style| %>
    <tr>
      <td><%= box_style.id %></td>
      <td><%= link_to box_style.name, "#", :class => "link_to_update" %></td>
    </tr>
  <% end %>
</table>
